; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "ReferenceRect",
"class": "CGChart",
"language": "GDScript",
"path": "res://addons/ChartGraph/core/chart.gd"
}, {
"base": "CGChart",
"class": "CGLine",
"language": "GDScript",
"path": "res://addons/ChartGraph/line_chart/line_chart.gd"
} ]
_global_script_class_icons={
"CGChart": "",
"CGLine": ""
}

[application]

config/name="PandemicSim3D"
run/main_scene="res://scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

CGUtilities="*res://addons/ChartGraph/core/utilities.gd"

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray( "ChartGraph" )

[rendering]

environment/default_environment="res://default_env.tres"
